{
    "name": "Magneto",
    "description": "Open magnets in peerflix",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "contextMenus", 
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    }
  }